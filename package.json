{
  "name": "socket.io-request",
  "version": "0.3.1",
  "description": "bidirectional request-response for socket.io",
  "main": "lib/index.js",
  "scripts": {
    "test": "./node_modules/.bin/run-s test:*",
    "test:mocha": "./node_modules/.bin/mocha test/*.js -r babel-polyfill --compilers js:babel-register",
    "test:standard": "./node_modules/.bin/standard",
    "build": "./node_modules/.bin/babel src/ --out-dir lib/ --source-maps inline",
    "watch": "npm run build -- --watch",
    "clean": "rm -rf lib/*"
  },
  "keywords": [
    "socket.io",
    "reqres",
    "request",
    "response"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^3.5.0",
    "npm-run-all": "^4.1.1",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "deserialize-error": "^0.0.3",
    "serialize-error": "^2.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shokai/socket.io-request.git"
  },
  "bugs": {
    "url": "https://github.com/shokai/socket.io-request/issues"
  },
  "homepage": "https://github.com/shokai/socket.io-request#readme"
}
